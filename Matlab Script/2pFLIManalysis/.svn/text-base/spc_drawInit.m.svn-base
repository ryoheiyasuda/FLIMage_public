function add_to_menu(roi_context);
global spc;
	item1 = uimenu(roi_context, 'Label', 'make new roi', 'Callback', 'spc_makeRoi');
    item2 = uimenu(roi_context, 'Label', 'select all', 'Callback', 'spc_selectAll');    
